if(passed_elec &amp;&amp; score&gt;=700 &amp;&amp; instance_number(spin_mine_parent)&lt;1 &amp;&amp; !global.elec_flag &amp;&amp; instance_number(cursor_bouncer_obj)&lt;1 &amp;&amp; instance_number(cannon)&lt;1 &amp;&amp; instance_number(dark_crystal_parent)&lt;1 &amp;&amp; instance_number(dark_medallion)&lt;1)
{ 
    passed_elec=0;
    global.elec_flag=1;
    if(irandom(1))//They will move down
    {
        with(instance_create(20,-250,elec_left_obj)) vspeed = 3;
        with(instance_create(room_width-20,-250,elec_right_obj)) vspeed = 3;
        instance_create(0,0,warning_obj);
    }
    else//They will move up
    {
        with(instance_create(20,room_height+250,elec_left_obj)) vspeed = -3;
        with(instance_create(room_width-20,room_height+250,elec_right_obj)) vspeed = -3;
        instance_create(0,room_height-40,warning_obj);
    }
    alarm[1] = room_speed*(25+irandom(20));
}


//Creating the bouncing mine
if(passed &amp;&amp; score&gt;=1300 &amp;&amp; !global.elec_flag &amp;&amp; instance_number(cannon)&lt;1 &amp;&amp; instance_number(spin_mine_parent)&lt;1 &amp;&amp; instance_number(cursor_bouncer_obj)&lt;1 &amp;&amp; instance_number(dark_crystal_parent)&lt;1 &amp;&amp; instance_number(dark_medallion)&lt;1)
{
    passed=0;
    instance_create(SSGM.x,SSGM.y,cursor_bouncer_obj);
    alarm[1] = room_speed*(25+irandom(20));
}
   
   
//Cannon
if(create &amp;&amp; score&gt;=1000 &amp;&amp; instance_number(dark_crystal_parent)&lt;1 &amp;&amp; !global.elec_flag &amp;&amp; instance_number(cursor_bouncer_obj)&lt;1 &amp;&amp; instance_number(spin_mine_parent)&lt;1 &amp;&amp; instance_number(cannon)&lt;1 &amp;&amp; instance_number(dark_medallion)&lt;1)
{
    create=0;
    global.dir=irandom(1);
    if(global.dir==1)
        var y0=room_height-150;
    else var y0=84;
    global.side=irandom(1);
    if(global.side)
        var x0=room_width+60
    else var x0=-60;
    instance_create(x0,y0,cannon);
    alarm[1] = room_speed*(25+irandom(20));       
}

//Bonus 
if(score-global.prev_score_bonus&gt;=3000)
{
    global.in_bonus = 1;
    alarm[0] = 1;//Triggering the alarm
}
/*Health pickup(added the else to prevent the health pickup from appearing
after the bonus flag was turned on)*/
else if(drop_health &amp;&amp; score&gt;50)
{
    drop_health = 0;
    alarm[9] = room_speed*100;
    instance_create(room_width/2,-40,hp_pickup);
}

//Handling the combo
if(global.combo&gt;=75)
    global.multi=4;

else if(global.combo&gt;=50)
    global.multi=2;
//Handling the combo










