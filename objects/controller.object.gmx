else if(irandom(100-bi*10)==0 &amp;&amp; score&gt;=250 &amp;&amp; instance_number(parent_crystal)&lt;global.limit_crystal &amp;&amp; instance_number(spin_crystal_parent)&lt;global.limit_spin_crystal)
