        if(!checkCollisionCircle(mine_parent,cx,yl,ccr) &amp;&amp; !checkCollisionCircle(mine_parent,cx,yu,ccr))
            instance_create(cx,cy,crystal);  
    
}

//Static crystals
else if(irandom(8)==0 &amp;&amp; instance_number(parent_crystal)&lt;global.limit_crystal)
{
    var chosen = irandom(3)
    if(chosen==0)//small_left_down_crystal
