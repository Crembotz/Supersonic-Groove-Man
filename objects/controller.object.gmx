        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Increasing the background scrolling speed 
time_over=1;//To prevent the creation of more game objects
audio_stop_sound(clock);
/*If there is a need to increase the scrolling speed for either changing the background
or entering the bonus*/
if(bi&lt;2 || global.in_bonus)
{
    //Waiting for the room to be empty 
    if(instance_number(parent_obstacle)== 0 &amp;&amp; instance_number(parent_crystal)== 0 &amp;&amp; !global.elec_flag &amp;&amp; instance_number(cannon)==0 
       &amp;&amp; instance_number(cursor_bouncer_obj)==0 &amp;&amp; instance_number(pickup_parent)==0)
    {
        
        if(background_drawer.scrolling_speed&lt;mspeed)
        {
            background_drawer.scrolling_speed++;
            alarm[0]=1;//Trigger this alarm again
        }
        else if(background_drawer.scrolling_speed==mspeed)//If the background reached max speed and it's to time to do the next action(begin bonus,change background or slow down scrolling speed)
        {
            background_drawer.scrolling_speed++;
            health_time = alarm[9];//Keeping the remaining time 
            alarm[9] = -1;//Stopping the alarm
            if(audio_is_playing(theme_slow)) audio_stop_sound(theme_slow);
            if(global.in_bonus)
                alarm[2]=room_speed*2.5;//Starting the bonus
            else alarm[5]=room_speed*4; 
        }
    }
    else alarm[0]=1;//To keep checking if the room is empty
}
/*If the current background is the last background and the player missed the bonus
just resume normal play*/
else alarm[5]=1;
</string>
