            <kind>1</kind>
            <string>///Return the background scrolling speed back to normal
if(background_drawer.scrolling_speed&gt;nspeed)
{
    if(!background_changed &amp;&amp; bi&lt;2)
    {
        setBackground(barray[bi++]); 
        background_changed = true;
    }
    background_drawer.scrolling_speed--;
    //Continue triggering this alarm until the background's speed returns back to normal
    if(background_changed)
        alarm[5]=3;//To create a better effect of moving to a different area
    else alarm[5]=1;
}
else//The background is now scrolling in normal speed
{
    background_changed = false;
    //To resume normal gameplay 
    if(global.in_bonus)
    {
        if(audio_is_playing(theme_fast))
            audio_stop_sound(theme_fast);
        with(bonus_on) instance_destroy();
        global.in_bonus=0;
    }
    global.prev_score_bonus = score;
    alarm[9] = health_time;
    with(SSGM) heat=0;
    alarm[0] = room_speed*(60*5-bi*20);
    time_over = 0;
}
